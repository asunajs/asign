{
  "required": [
    "token",
    "phone"
  ],
  "title": "移动云盘配置",
  "id": "#",
  "properties": {
    "token": {
      "description": "cookie authorization 字段",
      "type": "string",
      "id": "#token"
    },
    "phone": {
      "description": "手机号",
      "type": "string",
      "id": "#phone"
    },
    "auth": {
      "description": "auth_token",
      "type": "string",
      "id": "#auth"
    },
    "shake": {
      "description": "摇一摇配置",
      "type": "object",
      "id": "#shake",
      "properties": {
        "enable": {
          "description": "是否开启该功能",
          "id": "#shake/enable",
          "default": false,
          "type": "boolean"
        },
        "num": {
          "description": "摇一摇次数",
          "id": "#shake/num",
          "default": 15,
          "type": "number"
        },
        "delay": {
          "description": "每次间隔时间（秒）",
          "id": "#shake/delay",
          "default": 2,
          "type": "number"
        }
      },
      "default": {
        "enable": false,
        "num": 15,
        "delay": 2
      }
    },
    "garden": {
      "description": "果园配置",
      "type": "object",
      "id": "#garden",
      "properties": {
        "enable": {
          "description": "是否开启该功能",
          "id": "#garden/enable",
          "default": false,
          "type": "boolean"
        }
      },
      "default": {
        "enable": false
      }
    }
  },
  "type": "object",
  "default": {
    "shake": {
      "enable": false,
      "num": 15,
      "delay": 2
    },
    "garden": {
      "enable": false
    }
  }
}